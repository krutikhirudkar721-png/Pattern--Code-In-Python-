#1.

#index and find
    s="welcome to python programming"
    print(s.find("p"))#find first index  position of p ie: 11
    print(s.index("p"))#also same
    print(s.find("p",12))#find p from 12 index to end
    print(s.index("p",12))#same like find
    print(s.find("p",1,13))#find p from 1 to 12 index
    print(s.index("p",1,13))#same like find

#2.
#find vowels from string
    s1 = "Krutik"
    vowels = "aeiouAEIOU"
    vowel_count = 0
    for char in s1:
      if char in vowels:
        vowel_count += 1
    print(f"Number of vowels in '{s1}' is: {vowel_count}")

#3.
#split,partition
    s="welcome to all python programming "
    print(s.split()) #by default split from space and return answer in list
    print(s.split("p"))#jaha jaha p waha , ayega
#partition
#print(s.partition())#here must give value for partion nahi tao error
    print(s.partition("p"))
#p ke pahele ek part,then  p,then baki part give answer in tupple
#output=>('welcome to all ', 'p', 'ython programming ')
#jony jony wal aexample

#4.
#strip,rstrip,ltsrip

    s=" sbjain"
    print(s)# its print as it is
    print(s.lstrip())#remove left space

#rstrip
    s="sbjain     "
    print(s)
    print(s.rstrip())#its remove but nor see

#to see rstrip
    s="sbjain     "
    print(len(s))#its count all
    print(len(s.rstrip()))#after remove ight space

#5.
#access and print character from string
    str="Krutik"
    for i in str:
      print(i,end="")

#6.
#python program to print a string,extract characters from the string
    str="SB JAIN COLLAGE"
    print(str[::-1])
    print(str[0:])
    print(str[5:15])

#7.
#Program to print words with their length of a string.
    str="SB JAIN COLLAGE"
    print(str)
    print(len(str))

